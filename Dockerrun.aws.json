{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "nest-src",
      "host": {
        "sourcePath": "/var/app/current"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "nest-app",
      "image": "9995311896/nest-app",
      "memory": 2048,
      "essential": true,
      "mountPoints": [
        {
          "sourceVolume": "nest-src",
          "containerPath": "/opt/nest-src"
        }
      ],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3004
        }
      ],
      "command": ["yarn", "start:dev"]
    },
    {
      "name": "mongo",
      "image": "mongo:latest",
      "memory": 1024,
      "essential": true,
      "links": [
        "nest-app"
      ],
      "portMappings": [
        {
          "hostPort": 27017,
          "containerPort": 27017
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "nest-src",
          "containerPath": "/opt/node-src"
        }
      ]
    }
  ]
}
